<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vn.co.taxinet.mobile.newactivity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="16" />

    <!-- Maps permission -->
    <permission
        android:name="vn.co.taxinet.mobile.newactivity.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="vn.co.taxinet.mobile.newactivity.permission.MAPS_RECEIVE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- Required to show current location -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <!-- GCM connects to Internet Services. -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_INTERNET" />

    <!-- GCM requires a Google account. -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />

    <!-- Keeps the processor from sleeping when a message is received. -->
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <!-- Creates a custom permission so only this app can receive its messages. -->
    <permission
        android:name="vn.co.taxinet.mobile.newactivity.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="vn.co.taxinet.mobile.newactivity.permission.C2D_MESSAGE" />

    <!-- This app has permission to register and receive data message. -->
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <!-- Network State Permissions to detect Internet status -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <!-- Permission to vibrate -->
    <uses-permission android:name="android.permission.VIBRATE" />

    <!-- Required OpenGL ES 2.0. for Maps V2 -->
    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme" >

        <!-- Main Activity -->
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.MapActivity"
            android:icon="@drawable/ic_drawer"
            android:label="@string/app_name"
            android:launchMode="singleTop" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.StartActivity"
            android:icon="@color/facebook"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.LoginActivity"
            android:icon="@color/facebook"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.activity.ForgotPasswordActivity"
            android:icon="@color/facebook"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.ResetPasswordActivity"
            android:icon="@color/facebook"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.ProfileActivity"
            android:icon="@color/facebook"
            android:label="@string/app_name"
            android:parentActivityName="vn.co.taxinet.mobile.newactivity.ChangePasswordActivity" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.PayActivity"
            android:icon="@color/facebook"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.ChangePasswordActivity"
            android:icon="@color/facebook"
            android:label="@string/app_name"
            android:parentActivityName="vn.co.taxinet.mobile.newactivity.ResetPasswordActivity" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.CompanyActivity"
            android:icon="@color/facebook"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.PromotionTripActivity"
            android:icon="@color/facebook"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.VipRiderActivity"
            android:icon="@color/facebook"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.TripHistoryActivity"
            android:label="@string/app_name"
            android:parentActivityName="vn.co.taxinet.mobile.newactivity.ChangePasswordActivity" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.TripHistoryDetailsActivity"
            android:label="@string/app_name"
            android:parentActivityName="vn.co.taxinet.mobile.newactivity.TripHistoryActivity" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.PaymentActivity"
            android:label="@string/app_name"
            android:parentActivityName="vn.co.taxinet.mobile.newactivity.TripHistoryActivity" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.PromotionTripRidersActivity"
            android:label="@string/app_name"
            android:parentActivityName="vn.co.taxinet.mobile.newactivity.PromotionTripActivity" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.PromotionTripRiderItem"
            android:label="@string/app_name"
            android:parentActivityName="vn.co.taxinet.mobile.newactivity.PromotionTripActivity" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.AboutActivity"
            android:label="@string/app_name"
            android:parentActivityName="vn.co.taxinet.mobile.newactivity.MapActivity" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.SupportActivity"
            android:label="@string/app_name"
            android:parentActivityName="vn.co.taxinet.mobile.newactivity.MapActivity" >
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.PickAddressActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:parentActivityName="vn.co.taxinet.mobile.newactivity.PromotionTripActivity"
            android:windowSoftInputMode="stateHidden" >
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

            <!-- Points to searchable activity -->
            <meta-data
                android:name="android.app.default_searchable"
                android:value="vn.co.taxinet.mobile.newactivity.MainActivity" />

            <!-- Points to searchable meta data -->
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity
            android:name="vn.co.taxinet.mobile.newactivity.HomeAddressActivity"
            android:icon="@color/facebook"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:parentActivityName="vn.co.taxinet.mobile.newactivity.ProfileTripActivity"
            android:windowSoftInputMode="stateHidden" >
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>

            <!-- Points to searchable activity -->
            <meta-data
                android:name="android.app.default_searchable"
                android:value="vn.co.taxinet.mobile.newactivity.MainActivity" />

            <!-- Points to searchable meta data -->
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>

        <receiver
            android:name="vn.co.taxinet.mobile.gps.GcmBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />

                <category android:name="vn.co.taxinet.mobile.newactivity" />
            </intent-filter>
        </receiver>

        <service android:name="vn.co.taxinet.mobile.gps.GcmIntentService" />

        <provider
            android:name="vn.co.taxinet.mobile.gps.PlaceProvider"
            android:authorities="vn.co.taxinet.mobile.gps.PlaceProvider"
            android:exported="false" />

        <!-- Specifies the Android API Key, which is obtained from Google API Console -->
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyCVaEdcYFs1HYGExe6U2Ffbru1YfVA-WhQ" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>